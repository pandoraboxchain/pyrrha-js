{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"                    
                },
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins":[
        "@babel/syntax-object-rest-spread",
        "@babel/proposal-object-rest-spread"
    ],
    "sourceMaps": "inline"
}
