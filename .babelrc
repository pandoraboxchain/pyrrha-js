{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    // "browsers": ["last 2 versions", "safari >= 7"]
                    "node": "8.11.3"                    
                },
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins":[
        "@babel/plugin-syntax-object-rest-spread",
        "@babel/plugin-proposal-object-rest-spread"
    ],
    "sourceMaps": "inline"
}
